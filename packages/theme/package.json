{
  "name": "@vtex/shoreline-theme",
  "version": "0.1.1",
  "description": "Shoreline theme package",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": "vtex/shoreline",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsup src --format=esm,cjs --dts --no-splitting --shims"
  },
  "devDependencies": {
    "@types/cssesc": "3.0.0",
    "@types/fs-extra": "11.0.1",
    "@types/node": "20.4.5",
    "tsup": "7.1.0"
  },
  "dependencies": {
    "@vtex/shoreline-utils": "workspace:*",
    "autoprefixer": "10.4.14",
    "cssesc": "3.0.0",
    "fs-extra": "11.1.1",
    "jiti": "1.19.1",
    "postcss": "8.4.27",
    "postcss-nested": "6.0.1",
    "prettier": "3.0.0",
    "sucrase": "3.34.0"
  }
}
