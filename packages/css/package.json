  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.1",
    "@types/node": "20.14.9"
  },
  "dependencies": {
    "@vtex/shoreline-utils": "workspace:*",
    "browserslist": "4.23.1",
    "fs-extra": "11.1.1",
    "lightningcss": "1.25.1"
  }
}
