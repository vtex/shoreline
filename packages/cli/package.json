{
  "name": "@vtex/shoreline-cli",
  "version": "1.0.0-rc.8",
  "description": "Shoreline cli",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": "vtex/shoreline",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "bin": {
    "shoreline": "bin.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "src",
    "bin",
    "bin.js"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsup src --format=esm,cjs --dts --no-splitting --shims"
  },
  "devDependencies": {
    "@types/node": "20.4.5"
  },
  "dependencies": {
    "@vtex/shoreline-css": "^1.0.0-rc.8",
    "commander": "11.0.0"
  }
}
