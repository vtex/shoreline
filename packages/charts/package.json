  },
  "peerDependencies": {
    "@vtex/shoreline": "1.x",
    "echarts": "6.x",
    "react": "18.x",
    "react-dom": "18.x"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.9",
    "@vtex/shoreline": "workspace:*"
  },
  "dependencies": {
    "@vtex/shoreline-utils": "workspace:*",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.4",
    "lodash.clonedeep": "^4.5.0",
    "vitest-canvas-mock": "^0.3.3"
