{
  "name": "@vtex/shoreline-preset-admin",
  "version": "0.2.0",
  "description": "Shoreline preset admin package",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": "vtex/shoreline",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsup src --format=esm,cjs --dts --no-splitting --shims"
  },
  "devDependencies": {
    "@types/node": "20.10.0"
  },
  "dependencies": {
    "@vtex/shoreline-utils": "^0.5.0"
  }
}
